<h1>{{city.name}}</h1>
<div class="row">
  <div class="col-sm-12">
    <div id="img-banner">
      <img src={{city.photoUrl}} alt={{city.name}}/>
    </div>
  </div>
</div>
<hr>

<div class="row">
  {{#each events}}
    <div class="col-sm-4">
      <div id="img-box">
        <a href='/events/{{_id}}'>
        <img src={{photoUrl}} alt={{name}}>
        </a>
      </div>
      <h3>{{name}}</h3>
    </div>
  {{/each}}
</div>   