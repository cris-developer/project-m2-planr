<h2>Cities</h2>
<hr>

<div class="row">
  <h2>This {{user.firstName}} {{user.lastName}} has attended past events </h2>
  {{#each cities}}
    <div class="col-sm-4">
      <div id="img-box">
        <a href='/cities/{{_id}}/events'>
        <img src={{this.photoUrl}}/>
        </a>
      </div>
      <h3>{{this.name}}</h3>
    </div>
  {{/each}}
</div>   